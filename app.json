{
  "expo": {
    "name": "automirror",
    "slug": "automirror",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/mirror.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/mirror.png",
      "resizeMode": "contain",
      "backgroundColor": "#f6ac4b"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/mirror.png",
        "backgroundColor": "#f6ac4b"
      },
      "package": "com.pjg9905.automirror"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "c64cd527-c13e-40f0-9de0-6e6d645e5621"
      }
    },
    "plugins": [
      [
        "expo-build-properties", {
          "android": {
            "usesCleartextTraffic": true
          }
        }
      ]  
    ]
  }
}
